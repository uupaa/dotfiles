source .vimrc

set showtabline=2       " タブを常に表示
" set imdisable         " IMを無効化
set transparency=3      " 透明度を指定
set antialias
set guifont=Monaco:h12
" colorscheme macvim
colorscheme macvim

" dark にしたければ以下を有効に
" set background=dark
" light にしたければ以下を有効に
set background=light
colorscheme solarized

" 起動時にフルスクリーンモードにする
" set fuoptions=maxvert,maxhorz
" au GUIEnter * set fullscreen
set lines=60
set columns=160


